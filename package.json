  "scripts": {
    "prestart": "rimraf dist && cpx ./client/index.html ./dist/client/",
    "start": "node fuse --dev && node fuse",
    "test": "node -e \"require('./config/scripts').startTest()\"",
    "prod": "ngc -p tsconfig-aot.json",
    "e2e": "node -e \"require('./config/scripts').startE2E()\"",
    "e2e:prod": "node -e \"require('./config/scripts').startProdE2E()\"",
    "deploy:heroku": "node -e \"require('./config/scripts').herokuPrompt()\"",
    "cleanup": "node -e \"require('./config/helpers').cleanup()\""
  },
