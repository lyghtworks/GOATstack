{
    "compilerOptions": {
        "declaration": false,
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../../",
        "typeRoots": [
            "../../node_modules/@types"
        ],
        "types": [
          "core-js",
          "node",
          "lodash"
        ]
    },
    "compileOnSave": false,
    "exclude": [ ]
}
